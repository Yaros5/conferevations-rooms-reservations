@model Hospital?


@{
    ViewBag.Title = "title";
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/style_doctor.css">
    <title>Document</title>
</head>

<header>
    <div class = "fix_bug">
        <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
            <div class="container-fluid ">
                <h1 class="page header text-light">Online Hospital</h1>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <ul class="navbar-nav me-auto mb-2 mb-md-0">
                        <li class="nav-item">
                            <a class="nav-link disabled " aria-current="page" href="#">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" asp-controller="Doctor" asp-action="List">Doctor</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active " asp-controller="Hospital" asp-action="HospitalList">Hospital</a>
                        </li>
                    </ul>


                </div>
            </div>
        </nav>
    </div>
</header>
<div class="container flex-grow-1">
    <div class="card bug shadow my-5">
        <img src="@Model.image" alt="" class="card-img-top" >
        <div class="card-body">
            <h3 class="card-title">@Model.NameHospital</h3>
            <h4 class="text-secondary">@Model.ContactNumber</h4>
            <p class="card-text">WorK time: @Model.StartWork-@Model.FinishWork</p>


        </div>
    </div>
</div>


<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>        
<script src = "/js/script.js"></script>
  <footer id="bottom" class="bg-dark text-white py-3 flex-end">
    <div class="container text-center">
      <h6 class="my-auto">Online Hospital Â© Copyright 2022 Loyalty Point</h6>
    </div>
  </footer>

</html>